    <span class="hljs-comment">// Método para crear la circunferencia en SVG</span>
    <span class="hljs-title function_">dibujar</span>(<span class="hljs-params"></span>) {
        <span class="hljs-keyword">const</span> circunferencia = <span class="hljs-title function_">crearElementoSVG</span>(<span class="hljs-string">'circle'</span>, {
            <span class="hljs-attr">cx</span>: <span class="hljs-variable language_">this</span>.#centro.<span class="hljs-title function_">getX</span>(),
