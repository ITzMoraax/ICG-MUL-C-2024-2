<span class="hljs-keyword">function</span> <span class="hljs-title function_">calcularCentroide</span>(<span class="hljs-params">puntos</span>) {
    <span class="hljs-keyword">let</span> sumX = <span class="hljs-number">0</span>, sumY = <span class="hljs-number">0</span>;
