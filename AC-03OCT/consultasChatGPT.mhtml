    #punto2; <span class="hljs-comment">// Atributo privado</span>

    <span class="hljs-title function_">constructor</span>(<span class="hljs-params">punto1, punto2</span>) {
        <span class="hljs-variable language_">this</span>.#punto1 = punto1;
        <span class="hljs-variable language_">this</span>.#punto2 = punto2;
    }

    <span class="hljs-comment">// Método para crear la línea en SVG</span>
    <span class="hljs-title function_">dibujar</span>(<span class="hljs-params"></span>) {
        <span class="hljs-keyword">const</span> linea = <span class="hljs-title function_">crearElementoSVG</span>(<span class="hljs-string">'line'</span>, {
            <span class="hljs-attr">x1</span>: <span class="hljs-variable language_">this</span>.#punto1.<span class="hljs-title function_">getX</span>(),
