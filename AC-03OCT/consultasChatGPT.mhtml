    <span class="hljs-title function_">dibujar</span>(<span class="hljs-params">ctx</span>) {
        ctx.<span class="hljs-title function_">beginPath</span>();
        ctx.<span class="hljs-title function_">arc</span>(<span class="hljs-variable language_">this</span>.<span class="hljs-property">cx</span>, <span class="hljs-variable language_">this</span>.<span class="hljs-property">cy</span>, <span class="hljs-variable language_">this</span>.<span class="hljs-property">r</span>, <span class="hljs-number">0</span>, <span class="hljs-number">2</span> * <span class="hljs-title class_">Math</span>.<span class="hljs-property">PI</span>);
