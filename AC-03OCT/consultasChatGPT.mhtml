</code></div></div></pre><h3>Contenido de <code>vectorizado.js</code>:</h3><pre class="!overflow-visible"><div class="dark bg-gray-950 contain-inline-size rounded-md border-[0.5px] border-token-border-medium relative"><div class="flex items-center text-token-text-secondary bg-token-main-surface-secondary px-4 py-2 text-xs font-sans justify-between rounded-t-md h-9">javascript</div><div class="sticky top-9 md:top-[5.75rem]"><div class="absolute bottom-0 right-2 flex h-9 items-center"><div class="flex items-center rounded bg-token-main-surface-secondary px-2 font-sans text-xs text-token-text-secondary"><span class="" data-state="closed"><button class="flex gap-1 items-center py-1"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" class="icon-sm"><path fill-rule="evenodd" clip-rule="evenodd" d="M7 5C7 3.34315 8.34315 2 10 2H19C20.6569 2 22 3.34315 22 5V14C22 15.6569 20.6569 17 19 17H17V19C17 20.6569 15.6569 22 14 22H5C3.34315 22 2 20.6569 2 19V10C2 8.34315 3.34315 7 5 7H7V5ZM9 7H14C15.6569 7 17 8.34315 17 10V15H19C19.5523 15 20 14.5523 20 14V5C20 4.44772 19.5523 4 19 4H10C9.44772 4 9 4.44772 9 5V7ZM5 9C4.44772 9 4 9.44772 4 10V19C4 19.5523 4.44772 20 5 20H14C14.5523 20 15 19.5523 15 19V10C15 9.44772 14.5523 9 14 9H5Z" fill="currentColor"></path></svg>Copiar código</button></span></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="!whitespace-pre hljs language-javascript"><span class="hljs-keyword">class</span> <span class="hljs-title class_">Punto</span> {
    #x;
    #y;

    <span class="hljs-title function_">constructor</span>(<span class="hljs-params">x, y</span>) {
        <span class="hljs-variable language_">this</span>.#x = x;
        <span class="hljs-variable language_">this</span>.#y = y;
    }

    <span class="hljs-comment">// Métodos para acceder a los atributos encapsulados</span>
    <span class="hljs-title function_">getX</span>(<span class="hljs-params"></span>) {
        <span class="hljs-keyword">return</span> <span class="hljs-variable language_">this</span>.#x;
    }

    <span class="hljs-title function_">getY</span>(<span class="hljs-params"></span>) {
        <span class="hljs-keyword">return</span> <span class="hljs-variable language_">this</span>.#y;
    }

    <span class="hljs-comment">// Método para calcular el vector entre dos puntos</span>
    <span class="hljs-keyword">static</span> <span class="hljs-title function_">vector</span>(<span class="hljs-params">p1, p2</span>) {
        <span class="hljs-keyword">return</span> <span class="hljs-keyword">new</span> <span class="hljs-title class_">Punto</span>(p2.<span class="hljs-title function_">getX</span>() - p1.<span class="hljs-title function_">getX</span>(), p2.<span class="hljs-title function_">getY</span>() - p1.<span class="hljs-title function_">getY</span>());
    }

    <span class="hljs-comment">// Método para calcular el producto cruzado (determinante 2D)</span>
    <span class="hljs-keyword">static</span> <span class="hljs-title function_">productoCruzado</span>(<span class="hljs-params">p1, p2</span>) {
        <span class="hljs-keyword">return</span> (p1.<span class="hljs-title function_">getX</span>() * p2.<span class="hljs-title function_">getY</span>()) - (p1.<span class="hljs-title function_">getY</span>() * p2.<span class="hljs-title function_">getX</span>());
    }
}

<span class="hljs-keyword">function</span> <span class="hljs-title function_">dibujarPoligono</span>(<span class="hljs-params">puntos</span>) {
    <span class="hljs-keyword">const</span> svgCanvas = <span class="hljs-variable language_">document</span>.<span class="hljs-title function_">getElementById</span>(<span class="hljs-string">'svgCanvas'</span>);
    
    <span class="hljs-comment">// Crear el elemento polígono</span>
    <span class="hljs-keyword">let</span> poligono = <span class="hljs-variable language_">document</span>.<span class="hljs-title function_">createElementNS</span>(<span class="hljs-string">"http://www.w3.org/2000/svg"</span>, <span class="hljs-string">"polygon"</span>);
    
    <span class="hljs-comment">// Definir los puntos del polígono</span>
    <span class="hljs-keyword">let</span> puntosPoligono = puntos.<span class="hljs-title function_">map</span>(<span class="hljs-function"><span class="hljs-params">punto</span> =&gt;</span> <span class="hljs-string">`<span class="hljs-subst">${punto.getX()}</span>,<span class="hljs-subst">${punto.getY()}</span>`</span>).<span class="hljs-title function_">join</span>(<span class="hljs-string">" "</span>);
    
    <span class="hljs-comment">// Establecer los atributos del polígono</span>
    poligono.<span class="hljs-title function_">setAttribute</span>(<span class="hljs-string">"points"</span>, puntosPoligono);
    poligono.<span class="hljs-title function_">setAttribute</span>(<span class="hljs-string">"fill"</span>, <span class="hljs-string">"none"</span>);
    poligono.<span class="hljs-title function_">setAttribute</span>(<span class="hljs-string">"stroke"</span>, <span class="hljs-string">"black"</span>);
    
    <span class="hljs-comment">// Añadir el polígono al canvas SVG</span>
    svgCanvas.<span class="hljs-title function_">appendChild</span>(poligono);

    <span class="hljs-comment">// Determinar si el polígono es cóncavo o convexo</span>
    <span class="hljs-keyword">const</span> tipoPoligono = <span class="hljs-title function_">esConvexo</span>(puntos) ? <span class="hljs-string">"Convexo"</span> : <span class="hljs-string">"Cóncavo"</span>;
    <span class="hljs-variable language_">console</span>.<span class="hljs-title function_">log</span>(<span class="hljs-string">`El polígono es: <span class="hljs-subst">${tipoPoligono}</span>`</span>);
}

<span class="hljs-keyword">function</span> <span class="hljs-title function_">esConvexo</span>(<span class="hljs-params">puntos</span>) {
    <span class="hljs-keyword">let</span> signo = <span class="hljs-literal">null</span>;
    <span class="hljs-keyword">let</span> esConvexo = <span class="hljs-literal">true</span>;

    <span class="hljs-keyword">const</span> numPuntos = puntos.<span class="hljs-property">length</span>;
    
    <span class="hljs-keyword">for</span> (<span class="hljs-keyword">let</span> i = <span class="hljs-number">0</span>; i &lt; numPuntos; i++) {
        <span class="hljs-keyword">const</span> p0 = puntos[i];
        <span class="hljs-keyword">const</span> p1 = puntos[(i + <span class="hljs-number">1</span>) % numPuntos];
        <span class="hljs-keyword">const</span> p2 = puntos[(i + <span class="hljs-number">2</span>) % numPuntos];

        <span class="hljs-keyword">const</span> vector1 = <span class="hljs-title class_">Punto</span>.<span class="hljs-title function_">vector</span>(p0, p1);
        <span class="hljs-keyword">const</span> vector2 = <span class="hljs-title class_">Punto</span>.<span class="hljs-title function_">vector</span>(p1, p2);

        <span class="hljs-keyword">const</span> cruz = <span class="hljs-title class_">Punto</span>.<span class="hljs-title function_">productoCruzado</span>(vector1, vector2);

        <span class="hljs-keyword">if</span> (cruz !== <span class="hljs-number">0</span>) {
            <span class="hljs-keyword">if</span> (signo === <span class="hljs-literal">null</span>) {
                signo = <span class="hljs-title class_">Math</span>.<span class="hljs-title function_">sign</span>(cruz);
            } <span class="hljs-keyword">else</span> <span class="hljs-keyword">if</span> (signo !== <span class="hljs-title class_">Math</span>.<span class="hljs-title function_">sign</span>(cruz)) {
                esConvexo = <span class="hljs-literal">false</span>;
                <span class="hljs-keyword">break</span>;
            }
        }
    }
    
    <span class="hljs-keyword">return</span> esConvexo;
}

<span class="hljs-comment">// Ejemplo de lista de puntos</span>
<span class="hljs-keyword">const</span> puntos = [
    <span class="hljs-keyword">new</span> <span class="hljs-title class_">Punto</span>(<span class="hljs-number">100</span>, <span class="hljs-number">100</span>),
    <span class="hljs-keyword">new</span> <span class="hljs-title class_">Punto</span>(<span class="hljs-number">200</span>, <span class="hljs-number">100</span>),
    <span class="hljs-keyword">new</span> <span class="hljs-title class_">Punto</span>(<span class="hljs-number">250</span>, <span class="hljs-number">200</span>),
    <span class="hljs-keyword">new</span> <span class="hljs-title class_">Punto</span>(<span class="hljs-number">150</span>, <span class="hljs-number">250</span>),
    <span class="hljs-keyword">new</span> <span class="hljs-title class_">Punto</span>(<span class="hljs-number">50</span>, <span class="hljs-number">200</span>)
];

<span class="hljs-comment">// Dibujar el polígono</span>
<span class="hljs-title function_">dibujarPoligono</span>(puntos);
